<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="40.0" prefWidth="398.0" style="-fx-background-color: #999999;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FriendnameContainerController">
   <children>
      <ImageView fx:id="profilepic" fitHeight="35.0" fitWidth="35.0" layoutX="10.0" layoutY="2.5" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/userdefaultpic.png" />
         </image>
      </ImageView>
      <Label fx:id="friendname" layoutX="65.0" layoutY="5.5" text="User Name">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
